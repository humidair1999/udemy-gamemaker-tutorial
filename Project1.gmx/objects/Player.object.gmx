    }
}
else {
    if (up) {
        vspd = -16;
    }
    else {
        vspd = 0;
    }
}

if (right) {
    hspd = spd;
}

if (left) {
    hspd = -spd;
}

if (!right &amp;&amp; !left) {
    hspd = 0;
}

move(Solid);

